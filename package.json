{
  "name": "recent-switch",
  "displayName": "Recent switch",
  "version": "0.0.1",
  "description": "快速切换最近打开的标签",
  "author": "zac(zhuzhuyule@126.com)",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@plasmohq/messaging": "^0.7.1",
    "@plasmohq/storage": "^1.15.0",
    "plasmo": "0.90.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.4.11"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "postcss": "8.4.33",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "tabs",
      "bookmarks",
      "commands",
      "scripting",
      "storage"
    ],
    "commands": {
      "toggle-recent-tabs": {
        "suggested_key": {
          "default": "Ctrl+E",
          "mac": "Command+E"
        },
        "description": "快速在当前标签和最近使用的标签之间切换"
      },
      "open-search-ui": {
        "suggested_key": {
          "default": "Ctrl+O",
          "mac": "Ctrl+O"
        },
        "description": "打开最近标签/书签搜索界面"
      }
    }
  }
}
